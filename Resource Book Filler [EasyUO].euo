set %resourceBookId KLVZJMD
set %containerId XWNAKMD

loop:
findItem RMK_NMK_XMK_TMK_ZLK C_ , %containerId
if ( #findKind = -1 )
    pause
if ( #findType = ZLK && #findCol <> 0 )
{
    ignoreItem #findId
    goto loop
}
exevent drag #findId 900
exevent dropC #backPackId
wait 10
findItem RMK_NMK_XMK_TMK_ZLK C_ , #backPackId
set #lObjectId %resourceBookId
set #lTargetId #findId
set #lTargetKind 1
event macro 17
wait 10
click 356 324
target
event macro 22
goto loop

